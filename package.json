{
  "name": "remix-website",
  "version": "30.1.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "clean": "git clean -fdX .",
    "clean:safe": "del public/build .cache build prisma/dev.db",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build": "npm run build:css && remix build",
    "dev": "pm2-dev pm2.config.js",
    "dev:docs": "node -r ts-node/register/transpile-only -r dotenv/config scripts/watcher.ts",
    "format": "prettier --write ./",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "start": "NODE_ENV=production node server.js",
    "db:reset": "prisma migrate reset",
    "seed": "node -r ts-node/register/transpile-only --max-old-space-size=900 -r dotenv/config  scripts/seed.ts",
    "postseed": "node -r ts-node/register/transpile-only --max-old-space-size=900 -r dotenv/config scripts/verify-seed.ts"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.2",
    "@docsearch/react": "^3.3.2",
    "@mcansh/undoc": "0.8.0",
    "@prisma/client": "^3.5.0",
    "@reach/menu-button": "0.17.0",
    "@reach/rect": "0.17.0",
    "@reach/tabs": "0.17.0",
    "@remix-run/express": "^1.11.1",
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "@ryanflorence/md": "^7.2.0",
    "@ryanflorence/mdtut": "^1.2.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "accept-language": "^3.0.18",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cssnano": "^5.1.14",
    "date-fns": "^2.29.3",
    "dotenv": "^10.0.0",
    "emoji-regex": "^10.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "front-matter": "^4.0.2",
    "gray-matter": "^4.0.3",
    "lru-cache": "^6.0.0",
    "morgan": "^1.10.0",
    "p-queue": "^6.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "remark": "^13.0.0",
    "remark-html": "13.0.1",
    "remix": "^1.11.1",
    "semver": "^7.3.8",
    "ts-invariant": "^0.9.3",
    "ts-is-present": "^1.2.2",
    "ts-node": "^10.4.0",
    "tween-functions": "^1.2.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.11.1",
    "@types/lru-cache": "^5.1.1",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@types/semver": "^7.3.13",
    "chokidar": "^3.5.3",
    "del-cli": "^4.0.1",
    "fathom-client": "^3.5.0",
    "pm2": "^5.2.2",
    "postcss": "^8.4.21",
    "postcss-cli": "^9.0.2",
    "postcss-focus-visible": "^8.0.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.0.0",
    "prettier": "^2.8.3",
    "prisma": "^3.5.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=16"
  },
  "prettier": {}
}
