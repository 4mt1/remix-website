{
  "name": "remix-website",
  "sideEffects": false,
  "scripts": {
    "build:css": "npm run generate:css -- --env production",
    "build": "npm run build:css && remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:server": "cross-env NODE_ENV=development node --require dotenv/config server.js",
    "generate:css": "postcss styles --base styles --dir app/styles",
    "format": "prettier --write ./",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "start": "cross-env NODE_ENV=production node server.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.4",
    "@docsearch/react": "^3.3.4",
    "@remix-run/express": "^1.16.0",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@remix-run/serve": "^1.16.0",
    "@remix-run/v1-meta": "^0.1.1",
    "@remix-run/v1-route-convention": "^0.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "accept-language": "^3.0.18",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "dotenv": "^16.0.3",
    "emoji-regex": "^10.2.1",
    "escape-goat": "^4.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "fathom-client": "^3.5.0",
    "follow-redirects": "^1.15.2",
    "front-matter": "^4.0.2",
    "gray-matter": "^4.0.3",
    "gunzip-maybe": "^1.4.2",
    "iso-639-1": "^2.1.15",
    "lodash.merge": "^4.6.2",
    "lru-cache": "^8.0.5",
    "luxon": "^3.3.0",
    "morgan": "^1.10.0",
    "octokit": "^2.0.14",
    "parse-link-header": "^2.0.0",
    "parse-numeric-range": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-slug": "^7.0.1",
    "semver": "^7.5.0",
    "shiki": "^0.14.2",
    "tabbable": "^6.1.2",
    "tar": "^6.1.14",
    "tar-stream": "^3.0.0",
    "tiny-invariant": "^1.3.1",
    "ts-is-present": "^1.2.2",
    "tween-functions": "^1.2.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "yaml": "^2.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
