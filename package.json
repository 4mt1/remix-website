{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "clean": "git clean -fdX .",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build": "npm run build:css && remix build",
    "dev": "pm2-dev pm2.config.js",
    "dev:docs": "node -r ts-node/register/transpile-only -r dotenv/config scripts/watcher.ts",
    "format": "prettier --ignore-file .eslintignore --write ./",
    "postinstall": "remix setup",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "start": "remix-serve build",
    "prestart": "npm run seed",
    "db:reset": "prisma migrate reset",
    "seed": "node -r ts-node/register/transpile-only --max-old-space-size=900 -r dotenv/config  scripts/seed.ts && node -r ts-node/register/transpile-only --max-old-space-size=900 -r dotenv/config scripts/verify-seed.ts",
    "clean:safe": "del public/build .cache build prisma/dev.db"
  },
  "dependencies": {
    "@mcansh/undoc": "0.5.0",
    "@prisma/client": "^3.5.0",
    "@remix-run/react": "^0.21.0",
    "@remix-run/serve": "^0.21.0",
    "@ryanflorence/md": "7.0.1",
    "@ryanflorence/mdtut": "^1.2.1",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "accept-language": "^3.0.18",
    "clsx": "^1.1.1",
    "cssnano": "^5.0.11",
    "dotenv": "^10.0.0",
    "front-matter": "^4.0.2",
    "gray-matter": "^4.0.3",
    "p-queue": "6.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.2",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remix": "^0.21.0",
    "semver": "^7.3.5",
    "ts-invariant": "^0.9.3",
    "ts-is-present": "1.2.2",
    "ts-node": "^10.4.0",
    "tween-functions": "^1.2.0",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.21.0",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/semver": "^7.3.9",
    "@types/yaml": "1.9.7",
    "chokidar": "3.5.2",
    "del-cli": "4.0.1",
    "pm2": "^5.1.2",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-focus-visible": "^6.0.1",
    "postcss-nesting": "^10.0.1",
    "prettier": "^2.4.1",
    "prisma": "^3.5.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "16"
  },
  "sideEffects": false,
  "version": "2.0.2"
}
