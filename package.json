{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build": "npm run build:css && remix build",
    "dev": "pm2-dev pm2.config.js",
    "postinstall": "remix setup",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "start": "remix-serve build",
    "seed": "node -r ts-node/register/transpile-only --max-old-space-size=900 -r dotenv/config  scripts/seed.ts && node -r ts-node/register/transpile-only --max-old-space-size=900 -r dotenv/config scripts/verify-seed.ts"
  },
  "dependencies": {
    "@mcansh/undoc": "file:.yalc/@mcansh/undoc",
    "@prisma/client": "3.4.0",
    "@remix-run/react": "0.20.1",
    "@remix-run/serve": "0.20.1",
    "@ryanflorence/md": "7.0.0",
    "@tailwindcss/aspect-ratio": "0.3.0",
    "accept-language": "3.0.18",
    "clsx": "1.1.1",
    "dotenv": "10.0.0",
    "front-matter": "4.0.2",
    "gray-matter": "4.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "0.20.1",
    "semver": "7.3.5",
    "ts-invariant": "0.9.3",
    "ts-node": "10.4.0"
  },
  "devDependencies": {
    "@remix-run/dev": "0.20.1",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/semver": "7.3.9",
    "pm2": "5.1.2",
    "postcss": "8.3.11",
    "postcss-cli": "9.0.1",
    "postcss-focus-visible": "6.0.1",
    "postcss-nesting": "9.0.0",
    "prisma": "3.4.0",
    "tailwindcss": "2.2.19",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
